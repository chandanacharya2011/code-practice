class Solution {
    public String removeVowels(String S) {
        StringBuilder sb = new StringBuilder();
        for(int i=0; i<S.length(); i++){
            char character = S.charAt(i);
            if(character != 'a' && character != 'e' && character != 'i' 
               && character != 'o' && character != 'u' ){
                sb.append(character);
            }
        }
        return sb.toString();
    }
}
